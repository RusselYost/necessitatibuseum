{
  "name": "necessitatibuseum",
  "description": "Safely evaluate JavaScript (estree) expressions, sync and async.",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/RusselYost/necessitatibuseum.git",
  "author": "DarrinOrtiz",
  "repository": {
    "type": "git",
    "url": "https://github.com/RusselYost/necessitatibuseum.git"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "test": "mocha --recursive --ignore 'test/fixtures/**'",
    "test:ci": "nyc --reporter=lcov npm run test",
    "cover": "nyc --reporter=text --reporter=html npm run test"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.4",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/parser": "^7.15.5",
    "escodegen": "^2.0.0",
    "gulp-format-md": "^2.0.0",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0"
  },
  "keywords": [
    "@babel",
    "@babel/parser",
    "veritatis",
    "atque",
    "analysis",
    "nemo",
    "vel",
    "babel-cli",
    "compiler",
    "context",
    "ecma",
    "vero",
    "es6",
    "estree",
    "esprima",
    "eval",
    "rerum",
    "rerum",
    "debitis",
    "function",
    "invoke",
    "javascript",
    "voluptate",
    "in",
    "laudantium",
    "est",
    "parser",
    "parsing",
    "run",
    "safe",
    "rerum",
    "qui",
    "syntax",
    "tc39",
    "transpile",
    "transpiler",
    "tree"
  ],
  "verb": {
    "toc": true,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "whence"
      ]
    },
    "reflinks": [
      "acorn",
      "escodegen",
      "esprima"
    ],
    "lint": {
      "reflinks": true
    }
  }
}